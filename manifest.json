{
  "manifest_version": 2,
  "name": "Traducteur Lorrain",
  "version": "1.0",
  "description": "Permet de trôduire du français en Lorrain gros.",
  "icons": {
    "48": "icons/icons.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["traducteur-lorrain.js"],
      "match_about_blank": true,
      "all_frames": true
    }
  ]
}
